{
    "name": "laravel-bricks/flux",
    "description": "A description for flux.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Stefano Pimpolari",
            "email": "stefano.pimpolari@tten.it"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelBricks\\Flux\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelBricks\\Flux\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBricks\\Flux\\FluxServiceProvider"
            ],
            "aliases": {
                "Capacitor": "LaravelBricks\\Flux\\Facades\\Capacitor"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
